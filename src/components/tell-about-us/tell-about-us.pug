mixin tell-about-us(options)    ul.container.tell-about-us.pl-0(class= options.class)        - for (icon of options.data)            li.col-auto.tell-about-us__icon.pr-2.pl-0                a.tell-about-us__icon-link(href= icon.link)                    i.tell-about-us__icon-img(class= icon.img)