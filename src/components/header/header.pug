mixin header()    -        var beverages = [            {text: 'Главная', link: 'http://google.com'},            {text: 'Каталог', link: '#'},            {text: 'Доставка и оплата', link: '#'},            {text: 'О нас', link: '#'},            {text: 'Контакты', link: '#'},        ]    header#header        .header            .container.header__container                .row.header__bad-container.justify-content-between.py-2.align-items-center.mx-0                    .header__menu.col-auto.d-lg-none.m-0.p-0                        .header__menu-container.navbar.navbar-expand-lg.navbar-light.m-0.p-0                            button.header__menu-button.navbar-toggler.border-0(type='button' data-toggle='collapse' data-target='#headerNav' aria-controls='headerNav' aria-expanded='false' aria-label='Toggle navigation')                                span.header__menu-icon.navbar-toggler-icon                    #headerNav.col-auto.header__nav-container.row.collapse.navbar-collapse.d-lg-block.order-lg-0.order-3.mt-4.m-lg-0.mr-lg-1                        nav.header__nav.col-lg-12.border-success.px-0                            ul.row.header__beverages.px-0.justify-content-around.my-0.py-lg-3                                - for (el of beverages)                                    li.col-lg-auto.header__beverages-element.px-0.mb-4.mb-lg-0.mr-2                                        a.header__beverages-link(href=el.link)= el.text                    .header__logo.col-auto.px-0.justify-self-center                      a.header__logo-container(href="#")                        img.header__logo-image(src="img/logo-header.png")                    .header__phone.col-auto.px-0                        a.header__phone-number(href="tel:+79683934333") +7 (968) 393-43-33                    .header__cart.col-auto.ml-lg-3.mr-2                        a.header__cart-link(href="#")                            span.header__cart-count 2                            i.header__cart-image.fas.fa-shopping-basket(href="#")