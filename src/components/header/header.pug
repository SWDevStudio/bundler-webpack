mixin header()    -        var beverages = [            {text: 'Главная', link: 'http://google.com', icon: "fas fa-home"},            {text: 'Каталог', link: '#', icon: "fas fa-list-ul"},            {text: 'Доставка и оплата', link: '#', icon: "fas fa-truck"},            {text: 'О нас', link: '#', icon: "far fa-handshake"},            {text: 'Контакты', link: '#', icon: "fas fa-address-book"},        ]    header#header        .header            .container.header__container                .row.header__bad-container.justify-content-between.py-2.align-items-center.mx-0.flex-wrap                    .header__menu.col-auto.m-0.p-0.d-xl-none                        .header__menu-container.navbar.navbar-expand-xl.navbar-light.m-0.p-0                            button.header__menu-button.navbar-toggler.border-0(type='button' data-toggle='collapse' data-target='#headerNav' aria-controls='headerNav' aria-expanded='false' aria-label='Toggle navigation')                                span.header__menu-icon.navbar-toggler-icon                    nav#headerNav.header__nav.col-xl-auto.border-success.px-0.collapse.navbar-collapse-xl.header__nav-container.navbar.navbar-expand-xl.row.d-xl-block.order-xl-0.order-3.mt-4.m-xl-0.mr-xl-1                        ul.row.header__beverages.px-0.justify-content-around.my-0.py-xl-3                            - for (el of beverages)                                li.col-xl-auto.header__beverages-element.px-0.mb-4.mb-xl-0.mr-3                                    a.header__beverages-link(href=el.link)                                        i(class= el.icon).header__beverages-icon.d-xl-none                                        span.header__beverages-text= el.text                    .header__logo.col-auto.px-0.justify-self-center                      a.header__logo-container(href="#")                        img.header__logo-image(src="img/logo-header.png")                    .header__phone-and-cart.d-flex.align-items-center                        .header__phone.col-auto.px-0                            a.header__phone-number(href="tel:+79683934333") +7 (968) 393-43-33                        .header__cart.col-auto.ml-lg-3.mr-2                            a.header__cart-link(href="#")                                span.header__cart-count 2                                i.header__cart-image(href="#")(class='fa fa-shopping-basket')