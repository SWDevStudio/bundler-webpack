mixin header()    -        var beverages = [            {text: 'Виски', link: '#'},            {text: 'Водка', link: '#'},            {text: 'Пиво', link: '#'},            {text: 'Вино', link: '#'},            {text: 'Коньяк', link: '#'},            {text: 'Ром', link: '#'},            {text: 'Шампанское', link: '#'},            {text: 'Вермут', link: '#'},            {text: 'Текила', link: '#'},            {text: 'Ликер', link: '#'},        ]    header#header        .header            .container.header__container                .row.header__top.justify-content-between.py-3.align-items-center                    .header__menu.col-auto.d-lg-none.m-0.p-0                        .header__menu-container.navbar.navbar-expand-lg.navbar-light.m-0.p-0                            button.header__menu-button.navbar-toggler.border-0(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')                                span.header__menu-icon.navbar-toggler-icon                    .header__phone.col-auto.d-none.d-lg-block                        a.header__phone-number(href="tel:+79683934333") +7 (968) 393-43-33                    .header__logo.col-auto                        a.header__logo-container(href="#")                            img.header__logo-image(src="img/Night-Spirits.png")                            p.header__logo-text-container                                span.header__logo-text-heading Night Spirits                                span.header__logo-text-main Доставка алкоголя круглосуточно                        a.header__logo-phone.d-lg-none(href="tel:+79683934333") +7 (968) 393-43-33                    .header__cart.col-auto                        a.header__cart-link(href="#")                            i.header__cart-image.fas.fa-shopping-cart(href="#")                #navbarNav.row.header__bottom.collapse.navbar-collapse.d-lg-block                    nav.col-lg-12.header__bottom-container.border-success                        ul.row.header__beverages.px-5.justify-content-around.my-0.py-lg-3                            - for (el of beverages)                                li.col-lg-auto.header__beverages-element.px-0.mb-4.mb-lg-0                                    a.header__beverages-link(href=el.link)= el.text