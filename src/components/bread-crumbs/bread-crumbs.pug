mixin bread-crumbs(options)    .bread-crumbs(class= options.class + "__bread-crumbs")        ul.bread-crumbs__container-elements(class="").p-0            - for (crumbs of options.crumbs)                li.bread-crumbs__element                    a.bread-crumbs__link(href= crumbs.link)= crumbs.text